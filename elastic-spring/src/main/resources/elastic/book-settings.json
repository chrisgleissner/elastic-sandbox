{
  "index": {
    "number_of_shards": "1",
    "number_of_replicas": "0",
    "analysis": {
      "analyzer": {
        "default": {
          "tokenizer": "whitespace",
          "filter": [
            "custom_stop_word_filter"
          ]
        }
      },
      "filter": {
        "custom_stop_word_filter": {
          "type": "stop",
          "ignore_case": true,
          "stopwords": [
            "_customStopWords"
          ]
        }
      }
    }
  }
}